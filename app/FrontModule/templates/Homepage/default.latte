{layout ../@layout.latte}
{block content}
      <div class="container rental-container">
          <h1 class="title">
            {$page->name}
        </h1>
          <div class="row">
              <div class="col-md-6">
                  <article>
                    {block subcontent}
                    {$page->content|noescape}
                    {/block}
                  </article>
              </div>
              <div class="col-md-4">
                  <strong class="our-rentals">
                      Naše půjčovny:
                  </strong>
                <div class="our-rentals-box" id="our-rentals">
                    <ul class="list-unstyled">
                        <li>
                            <div id="heading-rental-prague" class="show-rental">
                                <a data-toggle="collapse" data-target="#rental-prague" aria-expanded="true" aria-controls="rental-prague">
                                    <i class="fa fa-map-marker"></i> Praha
                                </a>
                            </div>
                            <div id="rental-prague" class="collapse" aria-labelledby="heading-rental-prague" data-parent="#our-rentals">
                                <p>
                                    <a class="strong" href="https://www.polagraph.cz/">Polagraph</a>, Husinecká 14, Praha 3<br>
                                    po-pá 11.00 – 18.30
                                </p>
                            </div>
                        </li>
                        <!-- ./rental prague -->
                        <li>
                            <div id="heading-rental-brno" class="show-rental">
                                <a data-toggle="collapse" data-target="#rental-brno" aria-expanded="true" aria-controls="rental-brno">
                                    <i class="fa fa-map-marker"></i> Brno
                                </a>
                            </div>
                            <div id="rental-brno" class="collapse" aria-labelledby="heading-rental-brno" data-parent="#our-rentals">
                                <p>
                                    <a class="strong" href="https://www.polagraph.cz/">Polagraph</a>, Pekařská 34, Brno<br>
                                    po-pá 11.00 – 18.30
                                </p>
                            </div>
                        </li>
                        <!-- ./rental brno -->
                        <li>
                            <div id="heading-rental-ostrava" class="show-rental">
                                <a data-toggle="collapse" data-target="#rental-ostrava" aria-expanded="true" aria-controls="rental-ostrava">
                                    <i class="fa fa-map-marker"></i> Ostrava
                                </a>
                            </div>
                            <div id="rental-ostrava" class="collapse" aria-labelledby="heading-rental-ostrava" data-parent="#our-rentals">
                                <p>
                                    <a class="strong" target="_blank" href="http://www.artbookscoffee.sk/index.php/kontakt/">ArtBooksCoffee</a>, Baštová 6/B, Bratislava<br>
                                    po-pá 8.00 – 19.00, so 11.00 – 19.00
                                </p>
                            </div>
                        </li>
                        <!-- ./rental ostrava -->
                        <li>
                            <div id="heading-rental-bratislava" class="show-rental">
                                <a data-toggle="collapse" data-target="#rental-bratislava" aria-expanded="true" aria-controls="rental-bratislava">
                                    <i class="fa fa-map-marker"></i> Bratislava
                                </a>
                            </div>
                            <div id="rental-bratislava" class="collapse" aria-labelledby="heading-rental-bratislava" data-parent="#our-rentals">
                                <p>
                                    <a class="strong" target="_blank" href="https://www.cityfolklore.com/">City Folklore</a>, Nádražní 40, Ostrava<br>
                                    po-pá 10.00 – 18.00
                                </p>
                            </div>
                        </li>
                        <!-- ./rental bratislava -->
                    </ul>
                </div>
                <!-- ./our rentals -->
              </div>
        </div>
      </div>
      <!-- ./container -->
      <section class="rental">
        {snippet rent}
          <div class="container text-center">
              <ul class="list-inline rental-pagination">
                  <li id="rental-step-one" class="list-inline-item {if !empty($basket->step) && $basket->step >0}active{/if}">
                      1
                  </li>
                  <li id="rental-step-two" class="list-inline-item {if !empty($basket->step) && $basket->step >1}active{/if}">
                      2
                  </li>
                  <li id="rental-step-three" class="list-inline-item {if !empty($basket->step) && $basket->step >2}active{/if}">
                      3
                  </li>
                  <li id="rental-step-three" class="list-inline-item {if !empty($basket->step) && $basket->step >3}active{/if}">
                      4
                  </li>
              </ul>
              <div id="rental-step-zero" n:if="empty($basket->step) && empty($final)">
                  <h2 class="title">
                      Vyberte si typ fotoaparátu, <span>záleží na tom formát fotek</span>
                  </h2>
                  <div class="row">
                      <div class="col-md-6 col-lg-3" n:foreach="$cameras as $camera">
                          <div class="camera-type {$camera->alias}">
                              <span class="title">
                                  {$camera->name}
                              </span>
                              <div class="thumb">
                                  <div>
                                    {foreach $presenter->models->products->getPhotos($camera->id) as $im}
                                          <img n:if="$im->main == true" src="{$im|thumb:220:185:'FIT'}" alt="{$camera->name}">
                                          <img n:if="!$im->main == true" class="thumb-hover" src="{$im|thumb:220:185:'FIT'}" alt="{$camera->name}">
                                    {/foreach}
                                  </div>
                              </div>
                              <div class="description">
                                    {$camera->description|noescape}
                              </div>
                              <ul class="list-unstyled">
                                  <li>
                                      fotka {$camera->technical}
                                  </li>
                                  <li>
                                      {$camera->price} Kč
                                  </li>
                              </ul>
                              <a class="btn btn-primary to-step-one ajax" n:href="selectCamera! $camera->id" title="Půjčit {$camera->name}">
                                  Půjčit {$camera->name}
                              </a>
                          </div>
                      </div>
                  </div>
                  <!-- ./row -->
              </div>
              <!-- ./rental step zero -->
              <div id="rental-step-one" n:if="!empty($basket->step) && $basket->step==1">
                  <div class="camera-icon">
                      <img src="{$basket->camera|thumb:220:158:'FIT'}" alt="{$basket->camera->name}">
                      <span>
                          {$basket->camera->name}
                      </span>
                  </div>
                  <h2 class="title">
                      Kolik potřebujete fotek?
                  </h2>
                  <div id="number-of-photos-slider"></div>
                  <div class="number-of-photos">
                      <div class="float-left">
                          50 fotek
                      </div>
                      <div class="float-right">
                          150 fotek
                      </div>
                      <div class="clearfix"></div>
                  </div>
                  <p class="camera-description">
                      {strip_tags($basket->camera->description)|nl2br|noescape}
                  </p>
                  <a class="btn btn-primary btn-continue to-step-two ajax" n:href="setPictures!" title="Pokračovat na další krok">
                      Pokračovat
                  </a>
              </div>
              <!-- ./rental step one -->
              <div id="rental-step-two" n:if="!empty($basket->step) && $basket->step==2">
                  <div class="camera-icon">
                      <img src="{$basket->camera|thumb:220:158:'FIT'}" alt="{$basket->camera->name}">
                      <span>
                          {$basket->camera->name} a {$basket->amount} ks fotek
                      </span>
                  </div>
                  <h2 class="title">
                      Druhý krok: Vyberte si počet fotek <span>a napište své údaje</span>
                  </h2>
                  {form rentForm, class=>"rental-form ajax"}
                      <div class="row">
                        <div class="form-group col-md-6">
                            {label branch, class=>"required" /}
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <i class="fas fa-map-marker-alt"></i>
                                </div>
                                {input branch}
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            {label dates, class=>"required" /}
                            <div class="input-group">
                                <div class="input-group-addon">
                                    <i class="fa fa-calendar"></i>
                                </div>
                                {input dates}
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            {label name/}
                            {input name}
                        </div>
                        <div class="form-group col-md-6">
                            {label surname, class=>"required" /}
                            {input surname}
                        </div>
                        <div class="form-group col-md-6">
                            {label email, class=>"required" /}
                            {input email}
                        </div>
                        <div class="form-group col-md-6">
                            {label phone, class=>"required" /}
                            {input phone}
                        </div>
                        <div class="clearfix"></div>
                    </div>
                    {input submit}
                  {/form}
