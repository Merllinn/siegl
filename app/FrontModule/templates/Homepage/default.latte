                        "Únor",
                        "Březen",
                        "Duben",
                        "Květen",
                        "Červen",
                        "Červenec",
                        "Srpen",
                        "Září",
                        "Říjen",
                        "Listopad",
                        "Prosinec"
                    ],
                    "firstDay": 1
                }
              });

              $('input[name="dates"]').val('');
              $('input[name="dates"]').attr("placeholder","Datum půjčení - Datum vrácení");

                /* Volání AJAXu u všech odkazů s třídou ajax */
                $("a.ajax").on("click", function (event) {
                    event.preventDefault();
                    $("#load_dialog").show();
                    if($(this).hasClass("to-step-two")){
                        var link = this.href;
                        var amount = $('#number-of-photos-slider').slider("option", "value");
                        link = link+"&amount="+amount;
                        $.get(link);
                    }
                    else{
                        $.get(this.href);
                    }
                });

                /* AJAXové odeslání formulářů */
                $("form.ajax").on("submit", function () {
                    //$("#load_dialog").show();
                    $(this).ajaxSubmit();
                    return false;
                });

                $("form.ajax :submit").on("click", function () {
                    //$("#load_dialog").show();
                    $(this).ajaxSubmit();
                    return false;
                });


            });
            $('.related').not('.slick-initialized').slick({
                centerMode: true,
                centerPadding: '70px',
